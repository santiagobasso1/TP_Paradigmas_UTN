(adm buscarPorTitulo: claveB)]]</body></methods><methods><class-id>VClientePeliculas</class-id> <category>actions</category><body package="Protocols-Common" selector="botonReiniciar">botonReiniciaraspListaPeliculas list: adm peliculas.</body></methods><methods><class-id>VClientePeliculas</class-id> <category>actions</category><body package="Protocols-Common" selector="clickOnDataset">clickOnDataset	| pelicula |	(self wrapperAt: #botonFunciones) enable.	(self wrapperAt: #botonVerFicha) enable.	(self wrapperAt: #botonCalificaciones) enable.	pelicula := aspListaPeliculas selection.	pelicula isNil		ifFalse: 			[(self wrapperAt: #poster)				label: (ImageReader						fromFile: adm ruta , 'posters\' , pelicula id printString , '.jpg') image]</body></methods><methods><class-id>VClientePeliculas</class-id> <category>actions</category><body package="Protocols-Common" selector="botonCalificaciones">botonCalificaciones	| v p |	p := adm calificaciones				select: [:calificacion | calificacion idPelicula = aspPelicula value id].	v := VCalificaciones new.	v pelicula: aspPelicula value.	v adm: adm.	v usuario: usuario.	v aspListaCalificaciones list: p.	v open</body></methods><methods><class-id>VClientePeliculas</class-id> <category>actions</category><body package="Protocols-Common" selector="botonAyuda">botonAyuda| x |	x := VAyudaClientePelicula new.	x ruta: adm ruta.	x open.</body></methods><methods><class-id>VClientePeliculas</class-id> <category>actions</category><body package="Protocols-Common" selector="botonVerFicha">botonVerFicha| v |v:= VFicha new.v pelicula: self pelicula.v personas: adm personas.v ruta: adm ruta.v open.</body></methods><methods><class-id>VClientePeliculas</class-id> <category>imagenes</category><body package="Protocols-Common" selector="imagenFunciones">imagenFunciones^(ImageReader fromFile:(adm ruta), 'imagenes\funcion.png') image</body></methods><methods><class-id>VClientePelicul